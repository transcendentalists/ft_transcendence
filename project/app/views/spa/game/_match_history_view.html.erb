<script type="text/template" id="match-history-view-template">
  <div class="ui label">
    <%%= match_type %>
  </div>
  <div class="ui fluid buttons">
    <div class="ui left <%%= is_win ? 'positive' : '' %> button">
      <%%= score.player %>
    </div>
    <div class="or" data-text=<%%= is_win ? "승" : "패" %>></div>
    <div class="ui right <%%= is_win ? '' : 'red' %> button">
      <%%= score.enemy %>
    </div>
  </div>
  <div class="ui circular enemy image">
    <img class="ui tiny circular image" src="data:image/png;base64,">
  </div>
</script>
